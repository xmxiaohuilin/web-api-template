{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": [
      "app.js",
      "jsdoc_modules_list.js",
      "api",
      "config",
      "lib",
      "test"
    ]
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "encoding": "utf8",
    "template": "node_modules/docdash",
    "destination": "docs/docdash",
    "recurse": true,
    "verbose": true
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": false
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "default": {
      "outputSourceFiles": true,
      "includeDate": true
    }
  },
  "docdash": {
    "static": true,
    "sort": true
  }
}
